package org.example;

import org.junit.Test;
import static org.junit.Assert.*;

public class TestMathFunc {

    @Test
    public void testFunc() {
        double result = MathFunc.func(2);
        assertEquals(2.0, result, 0.001);
    }

    @Test
    public void testDf() {
        double result = MathFunc.Df(2);
        assertEquals(4.0, result, 0.001);
    }

    @Test
    public void testDichotomy() {
        double result = MathFunc.Dichotomy(0, 3);
        assertEquals(1.4142, result, 0.001);
    }

    @Test
    public void testQuickSort() {
        int[] testArr = {3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5};
        int[] expectedResult = {1, 1, 2, 3, 3, 4, 5, 5, 5, 6, 9};
        MathFunc.QuickSort(testArr);
        assertArrayEquals(expectedResult, testArr);
    }

    @Test
    public void testSelectionSort() {
        int[] testArr = {3, 1, 4, 1, 5, 9, 2, 6, 5, 3, 5};
        int[] expectedResult = {1, 1, 2, 3, 3, 4, 5, 5, 5, 6, 9};
        MathFunc.SelectionSort(testArr);
        assertArrayEquals(expectedResult, testArr);
    }
}
